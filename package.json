{
  "name": "podselectronapp",
  "version": "0.4.0",
  "private": true,
  "description": "PODS 3D APP",
  "main": "public/electron.js",
  "homepage": "./",
  "dependencies": {
    "@azure/msal-browser": "^3.20.0",
    "@azure/msal-react": "^2.0.22",
    "@babylonjs/core": "^7.10.2",
    "@babylonjs/loaders": "^7.10.2",
    "@babylonjs/serializers": "^7.10.2",
    "@emotion/react": "^11.11.4",
    "@emotion/styled": "^11.11.5",
    "@mui/icons-material": "^5.15.16",
    "@mui/material": "^5.15.16",
    "@react-oauth/google": "^0.12.1",
    "@testing-library/jest-dom": "^5.17.0",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^13.5.0",
    "3d-tiles-renderer": "^0.3.31",
    "aws-sdk": "^2.1628.0",
    "axios": "^1.7.2",
    "body-parser": "^1.20.2",
    "bootstrap": "^5.3.3",
    "collada2gltf": "^0.0.0",
    "crypto-js": "^4.2.0",
    "dxf": "^5.2.0",
    "express": "^4.19.2",
    "fbx2gltf": "^0.9.7-p1",
    "file-saver": "^2.0.5",
    "google-auth-library": "^9.13.0",
    "idb": "^8.0.0",
    "json-server": "^1.0.0-beta.1",
    "jwt-decode": "^4.0.0",
    "keycloak-js": "^25.0.2",
    "moment": "^2.30.1",
    "nodemailer": "^6.9.14",
    "paper": "^0.12.17",
    "react": "^18.3.1",
    "react-aad-msal": "^2.3.5",
    "react-bootstrap": "^2.10.2",
    "react-dom": "^18.3.1",
    "react-router-dom": "^6.23.0",
    "react-scripts": "5.0.1",
    "react-toastify": "^10.0.5",
    "request": "^2.88.2",
    "request-promise-native": "^1.0.9",
    "sqlite3": "5.1.6",
    "three": "^0.167.1",
    "uuid": "^9.0.1",
    "web-vitals": "^2.1.4",
    "ws": "^8.18.0",
    "xlsx": "^0.18.5"
  },
  "scripts": {
    "start": "electron .",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "author": "Jaleela Basheer",
  "license": "Proprietary software",
  "build": {
    "appId": "com.poulconsult.pods3dapp",
    "productName": "PODS",
    "win": {
      "target": [
        "nsis"
      ],
      "icon": "../build/icon"
    },
    "nsis": {
      "artifactName": "${description} Setup ${version}.${ext}",
      "uninstallDisplayName": "${description}",
      "shortcutName": "${description}",
      "createDesktopShortcut": true,
      "createStartMenuShortcut": true
    },
    "extraFiles": [
      "converters"
    ]
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "concurrently": "^8.2.2",
    "electron": "^30.0.2",
    "electron-builder": "^24.13.3"
  }
}
